{
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "printWidth": 80,
    "jsxSingleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": ["^react$", "^@?\\w", "^(@|components|utils|hooks)(/.*|$)", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
  }